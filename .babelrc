{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "debug": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "universal-import",
    [
      "transform-regenerator",
      {
        "asyncGenerators": false,
        "generators": false,
        "async": false
      }
    ],
    "syntax-async-functions"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
